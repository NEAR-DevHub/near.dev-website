{
  "buildCommand": "npm run build",
  "outputDirectory": "dist",
  "framework": "vite",
  "redirects": [
    {
      "source": "/josh",
      "permanent": true,
      "destination": "https://calendar.app.google/EH47q5uefTp7ekRVA"
    },
    {
      "source": "/josh-guille",
      "permanent": true,
      "destination": "https://calendar.app.google/ZM22zPZ3jkLR66kM9"
    },
    {
      "source": "/global-contracts",
      "permanent": true,
      "destination": "https://docs.near.org/smart-contracts/global-contracts"
    }
  ],
  "rewrites": [
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ]
}
